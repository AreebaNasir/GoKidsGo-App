<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".pGame1"
    android:background="@drawable/games"
    android:id="@+id/r1">
    <com.google.android.material.floatingactionbutton.FloatingActionButton
        android:id="@+id/pg1back"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@android:color/transparent"
        android:layout_marginLeft="15dp"
        android:layout_marginTop="15dp"
        android:backgroundTint="#FF1717"
        android:elevation="10dp"
        android:src="@drawable/circle_and_back"/>
    <com.google.android.material.floatingactionbutton.FloatingActionButton
        android:id="@+id/pg1music"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentRight="true"
        android:src="@drawable/music_and_circle"
        android:background="@android:color/transparent"
        android:layout_marginRight="15dp"
        android:layout_marginTop="15dp"
        android:backgroundTint="#79A5F2"
        android:elevation="10dp"
        />
    <com.google.android.material.floatingactionbutton.FloatingActionButton
        android:id="@+id/pg1settings"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/circle_and_settings"
        android:background="@android:color/transparent"
        android:layout_toLeftOf="@+id/pg1music"
        android:layout_marginTop="15dp"
        android:layout_marginRight="15dp"
        android:backgroundTint="#79A5F2"
        android:elevation="10dp"
        />

    <RelativeLayout
        android:layout_marginTop="80dp"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        >

        <ImageView
            android:id="@+id/cat"
            android:layout_width="140dp"
            android:layout_height="140dp"
            android:src="@drawable/cat"
            android:layout_marginLeft="40dp"
            android:layout_marginTop="80dp"/>

        <TextView
            android:id="@+id/c"
            android:layout_width="60dp"
            android:layout_height="140dp"
            android:layout_toRightOf="@id/cat"
            android:layout_marginTop="60dp"
            android:text="C"
            android:textSize="120dp"
            android:textColor="#FFFF"
            android:textStyle="bold"
            />
        <EditText
            android:id="@+id/a"
            android:layout_width="75dp"
            android:layout_height="140dp"
            android:layout_toRightOf="@id/c"
            android:layout_marginTop="60dp"
            android:textSize="105dp"
            android:textColor="#FFFF"
            android:textStyle="bold"
            />

        <TextView
            android:id="@+id/dash"
            android:layout_width="60dp"
            android:layout_height="140dp"
            android:layout_toEndOf="@id/c"
            android:layout_toRightOf="@id/c"
            android:layout_marginTop="60dp"
            android:text="_"
            android:textSize="120dp"
            android:textColor="#FFFF"
            android:textStyle="bold"
            android:layout_marginLeft="10dp"
            />

        <TextView
            android:id="@+id/t"
            android:layout_width="60dp"
            android:layout_height="140dp"
            android:layout_toRightOf="@id/a"
            android:layout_marginTop="60dp"
            android:text="T"
            android:textSize="120dp"
            android:textColor="#FFFF"
            android:textStyle="bold"
            />


        <Button
            android:id="@+id/pa"
            android:layout_width="60dp"
            android:layout_height="70dp"
            android:background="@drawable/customshadowbutton"
            android:text="A"
            android:textColor="#3700AD"
            android:textSize="35dp"
            android:fontFamily="@font/gagalin"
            android:layout_toLeftOf="@+id/pb"
            android:layout_marginTop="250dp"
            android:layout_marginRight="10dp"
            />

        <Button
            android:id="@+id/pb"
            android:layout_width="60dp"
            android:layout_height="70dp"
            android:background="@drawable/customshadowbutton"
            android:text="B"
            android:textColor="#3700AD"
            android:textSize="35dp"
            android:fontFamily="@font/gagalin"
            android:layout_toLeftOf="@+id/pc"
            android:layout_marginTop="250dp"
            android:layout_marginRight="10dp"

            />

        <Button
            android:id="@+id/pc"
            android:layout_width="60dp"
            android:layout_height="70dp"
            android:background="@drawable/customshadowbutton"
            android:text="C"
            android:textColor="#3700AD"
            android:textSize="35dp"
            android:layout_centerHorizontal="true"
            android:fontFamily="@font/gagalin"
            android:layout_marginTop="250dp"
            />

        <Button
            android:id="@+id/pd"
            android:layout_width="60dp"
            android:layout_height="70dp"
            android:background="@drawable/customshadowbutton"
            android:text="D"
            android:textColor="#3700AD"
            android:textSize="35dp"
            android:fontFamily="@font/gagalin"
            android:layout_marginLeft="10dp"
            android:layout_toRightOf="@id/pc"
            android:layout_marginTop="250dp"
            />
        <Button
            android:id="@+id/pe"
            android:layout_width="60dp"
            android:layout_height="70dp"
            android:background="@drawable/customshadowbutton"
            android:text="E"
            android:textColor="#3700AD"
            android:textSize="35dp"
            android:fontFamily="@font/gagalin"
            android:layout_marginLeft="10dp"
            android:layout_toRightOf="@id/pd"
            android:layout_marginTop="250dp"
            />

        <Button
            android:id="@+id/pf"
            android:layout_width="60dp"
            android:layout_height="70dp"
            android:background="@drawable/customshadowbutton"
            android:text="F"
            android:textColor="#3700AD"
            android:textSize="35dp"
            android:fontFamily="@font/gagalin"
            android:layout_marginRight="10dp"
            android:layout_toLeftOf="@+id/pg"
            android:layout_marginTop="340dp"
            />

        <Button
            android:id="@+id/pg"
            android:layout_width="60dp"
            android:layout_height="70dp"
            android:background="@drawable/customshadowbutton"
            android:text="G"
            android:textColor="#3700AD"
            android:textSize="35dp"
            android:fontFamily="@font/gagalin"
            android:layout_toLeftOf="@+id/ph"
            android:layout_marginTop="340dp"
            android:layout_marginRight="10dp"

            />

        <Button
            android:id="@+id/ph"
            android:layout_width="60dp"
            android:layout_height="70dp"
            android:background="@drawable/customshadowbutton"
            android:text="H"
            android:textColor="#3700AD"
            android:textSize="35dp"
            android:layout_centerHorizontal="true"
            android:fontFamily="@font/gagalin"
            android:layout_marginTop="340dp"
            />

        <Button
            android:id="@+id/pi"
            android:layout_width="60dp"
            android:layout_height="70dp"
            android:background="@drawable/customshadowbutton"
            android:text="I"
            android:textColor="#3700AD"
            android:textSize="35dp"
            android:fontFamily="@font/gagalin"
            android:layout_marginLeft="10dp"
            android:layout_toRightOf="@id/ph"
            android:layout_marginTop="340dp"
            />
        <Button
            android:id="@+id/pj"
            android:layout_width="60dp"
            android:layout_height="70dp"
            android:background="@drawable/customshadowbutton"
            android:text="J"
            android:textColor="#3700AD"
            android:textSize="35dp"
            android:fontFamily="@font/gagalin"
            android:layout_marginLeft="10dp"
            android:layout_toRightOf="@id/pi"
            android:layout_marginTop="340dp"
            />


    </RelativeLayout>

    <ImageView
        android:id="@+id/shypooh"
        android:layout_width="148dp"
        android:layout_height="179dp"
        android:layout_alignParentRight="true"
        android:layout_alignParentBottom="true"
        android:layout_marginRight="251dp"
        android:layout_marginBottom="-1dp"
        android:src="@drawable/pooh_shy" />

    <ImageView
        android:id="@+id/bluecloud"
        android:layout_width="159dp"
        android:layout_height="105dp"
        android:layout_above="@id/shypooh"
        android:layout_alignParentRight="true"
        android:layout_marginRight="155dp"
        android:layout_marginBottom="-70dp"
        android:background="@drawable/bluecloud"
        android:gravity="center" />

    <TextView
        android:id="@+id/sen2"
        android:layout_width="95dp"
        android:layout_height="wrap_content"
        android:layout_above="@id/shypooh"
        android:layout_alignParentRight="true"
        android:layout_marginRight="186dp"
        android:layout_marginBottom="-44dp"
        android:fontFamily="@font/atmabold"
        android:gravity="center"
        android:text="Fill in the Blanks"
        android:textColor="#3933B5"></TextView>


</RelativeLayout>
